rm(LAD, CCG, Counties, Region, England, UA)
write.csv(lookup, "~/GBD/Area_lookup_table.csv", row.names = FALSE)
write.csv(Areas, "~/GBD/Area_types_table.csv", row.names = FALSE)
}
